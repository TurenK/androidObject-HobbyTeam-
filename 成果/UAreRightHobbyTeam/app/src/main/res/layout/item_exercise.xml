<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linearww656"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/relativelay5161"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linear5616516"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/back"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linear1"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:orientation="horizontal"
                android:paddingTop="5dp">

                <LinearLayout
                    android:id="@+id/linearww"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:orientation="horizontal" />

                <LinearLayout
                    android:id="@+id/linearww1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:id="@+id/empty22"
                        android:layout_width="20dp"
                        android:layout_height="0dp"
                        android:orientation="horizontal" />

                    <TextView
                        android:id="@+id/exercise_item_name"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.7"
                        android:textColor="@color/black"/>

                    <ImageView
                        android:id="@+id/exercise_item_edit"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="0.1"
                        android:background="@mipmap/ellipsis_icon" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearww5"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:orientation="horizontal" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearww6"
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:orientation="horizontal"
                android:paddingTop="8dp">

                <LinearLayout
                    android:id="@+id/linearww8"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.9"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/linearww10"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="0.65"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/linearww11"
                            android:layout_width="match_parent"
                            android:layout_height="160dp"
                            android:gravity="center">

                            <ImageView
                                android:id="@+id/img_exercise_item"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearww12"
                            android:layout_width="match_parent"
                            android:layout_height="35dp"
                            android:gravity="center"
                            android:orientation="horizontal"
                            android:paddingBottom="5dp"
                            android:paddingTop="5dp">

                            <LinearLayout
                                android:id="@+id/empty23"
                                android:layout_width="5dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal" />

                            <TextView
                                android:id="@+id/exercise_item_tag1"
                                android:layout_width="80dp"
                                android:layout_height="match_parent"
                                android:background="@color/back"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                            <LinearLayout
                                android:id="@+id/empty21"
                                android:layout_width="5dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal" />

                            <TextView
                                android:id="@+id/exercise_item_tag2"
                                android:layout_width="80dp"
                                android:layout_height="match_parent"
                                android:background="@color/back"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                            <LinearLayout
                                android:id="@+id/empty24"
                                android:layout_width="5dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal" />

                            <TextView
                                android:id="@+id/exercise_item_tag3"
                                android:layout_width="80dp"
                                android:layout_height="match_parent"
                                android:background="@color/back"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                            <LinearLayout
                                android:id="@+id/empty25"
                                android:layout_width="5dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal" />

                            <TextView
                                android:id="@+id/exercise_item_tag4"
                                android:layout_width="80dp"
                                android:layout_height="match_parent"
                                android:background="@color/back"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="14sp" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearww16"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/linearww17"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.05"
                    android:orientation="horizontal" />

                <LinearLayout
                    android:id="@+id/linearww18"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.9"
                    android:orientation="horizontal"
                    android:paddingBottom="5dp">

                    <LinearLayout
                        android:id="@+id/linearww19"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/exercise_item_likeImg"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:background="@mipmap/like_before" />

                        <TextView
                            android:id="@+id/exercise_item_likeText"
                            android:layout_width="40dp"
                            android:layout_height="20dp"
                            android:gravity="center"
                            android:text="0"
                            android:textColor="@color/black"
                            android:textSize="15sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearww20"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/exercise_item_commentImg"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:background="@mipmap/comment" />

                        <TextView
                            android:id="@+id/exercise_item_commentText"
                            android:layout_width="40dp"
                            android:layout_height="20dp"
                            android:gravity="center"
                            android:text="0"
                            android:textColor="@color/black"
                            android:textSize="15sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearww21"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center">

                        <ImageView
                            android:id="@+id/exercise_item_addteamImg"
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:background="@mipmap/team_small_icon" />

                        <TextView
                            android:id="@+id/exercise_item_addteamText"
                            android:layout_width="40dp"
                            android:layout_height="20dp"
                            android:gravity="center"
                            android:text="详情"
                            android:textColor="@color/black"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearww22"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.05"
                    android:orientation="horizontal" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linear45151"
            android:layout_width="match_parent"
            android:layout_height="15dp"
            android:layout_below="@id/linear5616516"
            android:background="@color/colorGray">

            <ImageView
                android:id="@+id/separate_line_item"
                android:layout_width="match_parent"
                android:layout_height="15dp"
                android:background="@color/aliceblue"
                android:src="@mipmap/separate_line_items" />
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
